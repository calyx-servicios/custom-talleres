<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_change_production_qty_wizard" model="ir.ui.view">
            <field name="name">Change Quantity To Produce - Inherit</field>
            <field name="model">change.production.qty</field>
            <field name="inherit_id" ref="mrp.view_change_production_qty_wizard"/>
            <field name="arch" type="xml">
            	<xpath expr="//field[@name='product_qty']" position="attributes">
	        		<attribute name="widget">integer</attribute>
	        	</xpath>
            </field>
        </record>
    </data>
</odoo>    
