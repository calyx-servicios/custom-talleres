<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="product_template_immediate_delivery_form_view" model="ir.ui.view">
        <field name="name">product.template.immediate.delivery.form.view</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook/page[@name='general_information']/group/group[@name='group_general']/field[@name='categ_id']" position="after">
                <field name="immediate_delivery"/>
            </xpath>
        </field>
    </record>
</odoo>