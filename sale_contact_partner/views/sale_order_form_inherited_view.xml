<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="sale_order_contact_form" model="ir.ui.view">
        <field name="name">sale.order.contact.inherited.form</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale_order_workflow.sale_order_form_design"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='picking_status']" position="after">
                <field name="partner_zone" string="Partner Zone" readonly="1" force_save="1"/>
            </xpath>
        </field>
    </record>
</odoo>